{
  "name": "u-bethel",
  "version": "1.0.0",
  "description": "u-Bethel home page",
  "main": "app.js",
  "author": "JunO",
  "dependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.0",
    "@babel/node": "^7.8.0",
    "@babel/polyfill": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@babel/runtime": "^7.8.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "helmet": "^3.21.2",
    "ip": "^1.1.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "ngrok": "^3.2.7",
    "node-instagram": "^4.3.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "prisma-client-lib": "^1.34.10",
    "pug": "^2.0.4",
    "sass-loader": "^8.0.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --ignore 'scss'",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:server": "babel src --out-dir build --ignore src/assets,src/static,src/webpack.config.js ",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "copyAll": "cp -R src/views build && cp -R src/static build && cp -R src/icon_img build && cp -R src/font build",
    "prisma": "prisma deploy",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "node build/init.js"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}

extends layouts/main

block content
    .image-test--home
        .main-img.slider
            .main-img-black
            img#jsMainImg
        img.main-img__left#jsArrowLeft(src="icon-img/arrow_left.png")
        img.main-img__right#jsArrowRight(src="icon-img/arrow_right.png")
    .home__article
        .primary__section-one
            .primary__title
                span Primary Menu
                .primary__title-submain
                    .primary__subtitle
                        span 벧엘교회에 오신 여러분을 진심으로 환영 합니다.
                    .primary__description
                        span 벧엘교회는 썰물로 갯벌에 박혀버린 인생을 은혜로 배 띄우려합니다. 이것은 그리스도로 말미암아 되는 것이고, #[strong 참 신앙으로 이루어져야 한다고 믿습니다.]
        .primary__section-two
            .primary-box
                .primary__number 01
                .primary__semititle
                    span 담임목사님 소개
                    span 안녕하세요? 주의 이름으로 #[strong 환영 합니다!]
                    a(href="")
                        .primary-hoverbox
                            img(src="icon-img/go_icon.png")
                            span 바로가기
                        img(src="icon-img/Layer5.png")
            .primary-box
                .primary__number 02
                .primary__semititle 
                    span 예배안내
                    span 예배는 하나님과 우리 인간의 #[strong 만남의 시간]입니다.
                    a(href="")
                        .primary-hoverbox
                            img(src="icon-img/go_icon.png")
                            span 바로가기
                        img(src="icon-img/Layer7.png")
            .primary-box
                .primary__number 03
                .primary__semititle 
                    span 새가족환영
                    span 여러분의 #[strong 마음의 쉼터와 은혜의 공간]이 되길 소망합니다.
                    a(href="")
                        .primary-hoverbox
                            img(src="icon-img/go_icon.png")
                            span 바로가기
                        img(src="icon-img/Layer6.png")
        .primary__section-three
                .primary__guide
                    .guide-box
                        .guide-bgbox.bgbox-i
                        a(href="")
                            .guide-hoverbox
                                img(src="icon-img/go_icon_2.png")
                                span 일산안내 Go
                            img.guide-img(src="icon-img/일산성전_LINK.png")
                        span 일산 (#[strong i]) 안내
                    .guide-box
                        .guide-bgbox.bgbox-u
                        a(href="")
                            .guide-hoverbox
                                img(src="icon-img/go_icon_2.png")
                                span 운정안내 Go
                            img.guide-img(src="icon-img/운정성전_LINK.png")
                        span 운정 (#[strong u]) 안내
    .home__article-second
        .home__bethelstory
            .home__bethelstory__section-one
                .bethelstory-title
                    span 벧엘 스토리
                    .bethelstory-description
                        span 
                        span Bethel Story
                        span.bethelstory-nav
                            span + Story More
                            span 벧엘 주보 (전체보기)
                .bethelstory-main
                    .bethelstory-contents
                        .bethelstory-contents__img
                            img(src="icon-img/성탁축하공연copy.png")
                        img.bethelstory-left(src="icon-img/arrow_left.png")
                        img.bethelstory-right(src="icon-img/arrow_right.png")
                        img.playbtn-red(src="icon-img/play_btn.png")
            .home__bethelstory__section-two
                    
                        
                